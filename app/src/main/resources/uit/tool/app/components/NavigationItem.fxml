<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>

<?import javafx.scene.image.Image?>


<fx:root xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
         stylesheets="@/uit/tool/app/stylesheets/NavigationItem.css" styleClass="cont"
         type="javafx.scene.layout.HBox" alignment="CENTER" layoutX="10.0" layoutY="10.0"
         fx:controller="uit.tool.app.components.NavigationItem" fx:id="container">
    <children>

        <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true" fx:id="icon">
            <!--            <image>-->
            <!--                <Image url="@/uit/tool/app/assets/black-square.png"/>-->
            <!--            </image>-->
            <viewport>
                <Rectangle2D/>
            </viewport>
        </ImageView>
        <Button fx:id="btn" alignment="CENTER" maxWidth="-Infinity" minWidth="-Infinity"
                mnemonicParsing="false" prefHeight="32.0" prefWidth="128.0" text="_">
            <font>
                <Font name="Arial Bold Italic" size="18.0"/>
            </font>
        </Button>
    </children>
    <padding>
        <Insets top="10" bottom="10"/>
    </padding>
</fx:root>
