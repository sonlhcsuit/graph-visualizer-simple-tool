<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.image.Image?>


<fx:root type="javafx.scene.layout.HBox" alignment="CENTER" layoutX="10.0" layoutY="10.0" fx:id="btnGroupNew" fx:controller="uit.tool.app.components.NavigationItem">
    <children>
        <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@/uit/tool/app/assets/contract.png"/>
            </image>
            <viewport>
                <Rectangle2D/>
            </viewport>
        </ImageView>
        <Button fx:id="btnNew" alignment="CENTER" maxWidth="-Infinity" minWidth="-Infinity"
                mnemonicParsing="false" prefHeight="32.0" prefWidth="128.0"
                style="-fx-background-color: none;" text="New" textFill="#1ca7ec">
            <font>
                <Font name="Arial Italic" size="18.0"/>
            </font>
        </Button>
    </children>
    <VBox.margin>
        <Insets bottom="10.0" top="10.0"/>
    </VBox.margin>
</fx:root>
